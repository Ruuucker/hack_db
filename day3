//9
void ft_sort_integer_table(int *tab, int size)
{
	int sorted;
	int z = 0;
	int x = 0;

	while (*tab[size] != '\0')
	{
		if (*tab[size] < z)
		{
			sorted[z] = *tab[size];
		}
		size--;
		z++;
	}
}
